#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
char ch;
char s[20];
char sen[200];

//{use printf while working with another IDE expect Hackerank}
//printf("enter a character: ");
scanf("%c\n",&ch);

//printf("enter a string: ");
scanf("%s\n",&s);   

//printf("enter a sentence: ");
scanf("%[^\n]s",&sen);

printf("%c\n",ch);
printf("%s\n",s);
printf("%s\n",sen);
return 0;
}
